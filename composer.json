{
  "name": "gepur-it/action-logger-bundle",
  "description": "Action logger bundle",
  "type": "symfony-bundle",
  "keywords": [
    "symfony",
    "mongodb",
    "logs",
    "logger",
    "erp"
  ],
  "homepage": "https://github.com/gepur-it/action-logger-bundle",
  "require": {
    "php": "^7.0",
    "symfony/dependency-injection": ">=4.0",
    "symfony/config": ">=4.0",
    "symfony/yaml": ">=4.0",
    "symfony/http-kernel": ">=4.0",
    "symfony/expression-language": "^4.0",
    "doctrine/mongodb-odm": "^1.1",
    "doctrine/mongodb-odm-bundle": "^3.4.1",
    "symfony/security-bundle": "^4.0",
    "doctrine/doctrine-fixtures-bundle": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3",
    "symfony/phpunit-bridge": "^3.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Andrii Yakovlev",
      "email": "yawa20@gmail.com"
    },
    {
      "name": "Marina Mileva",
      "email": "m934222258@gmail.com"
    },
    {
      "name": "Viktor Pavlov",
      "email": "zogxray@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "GepurIt\\ActionLoggerBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "provide": {
    "ext-mongo": "1.6.14"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
